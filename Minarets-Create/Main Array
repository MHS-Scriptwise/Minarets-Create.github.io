from PIL import Image
import math
gsize = 600
img = image.new('RGB', (600,600), "white")
Canvas_Array = []
for i in range(gsize**2):
  Canvas_Array.clear()
  color = img.getpixel((i%gsize,math.floor(i/gsize))
  R = color[0]
  G = color[1]
  B = color[2]
  Canvas_Array.append([R,G,B])
for i in range(100):
  print(Canvas_Array[i])
