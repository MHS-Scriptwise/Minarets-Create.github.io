from PIL import Image
import turtle
import math
gsize = 1000
tt = 10
t.pensize(tt)
img = image.new('RGB', (1000,1000), "white")
Canvas_Array = []
def Canvas_Update():
  for i in range(gsize**2):
    Canvas_Array.clear()
    color = img.getpixel((i%gsize,math.floor(i/gsize))
    R = color[0]
    G = color[1]
    B = color[2]
    Canvas_Array.append([R,G,B])
for i in range(100):
  print(Canvas_Array[i])
